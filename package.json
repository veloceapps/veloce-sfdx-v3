{
  "name": "veloce-sfdx-v3",
  "description": "Extension to sfdx which allows veloce specific data pull and push",
  "version": "0.0.9",
  "author": "veloceapps",
  "bugs": "https://github.com/veloceapps/veloce-sfdx-v3/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/apex-node": "^0.11.0",
    "@salesforce/command": "^4",
    "@salesforce/core": "^2",
    "axios": "^0.27.1",
    "chokidar": "^3.5.3",
    "open": "^8.2.1",
    "ts-replace-all": "^1.0.0",
    "tslib": "^2",
    "uuid": "^8.3.2",
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^2",
    "@salesforce/dev-scripts": "^0",
    "@salesforce/prettier-config": "^0",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/jsforce": "^1.9.29",
    "@types/mocha": "^8",
    "@types/node": "^18.0.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "csv-parse": "^4.15.3",
    "csv-write-stream": "^2.0.0",
    "csv-writer": "^1.6.0",
    "eslint": "^7",
    "eslint-config-oclif": "^3.1",
    "eslint-config-prettier": "^8",
    "eslint-config-salesforce": "^0",
    "eslint-config-salesforce-typescript": "^0",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "@types/folder-hash": "^4.0.2",
    "folder-hash": "^4.0.2",
    "globby": "^11",
    "husky": "^4",
    "mocha": "^8",
    "nyc": "^15",
    "prettier": "^2",
    "pretty-quick": "^3",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/veloceapps/veloce-sfdx-v3",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "veloceapps/veloce-sfdx-v3",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts src/commands/veloce/**/*.ts test/**/*.ts",
    "fix": "eslint src/**/*.ts src/commands/veloce/**/*.ts test/**/*.ts --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha -r ts-node/register --timeout 30000 --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "pretty-quick": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick && npm run fix"
    }
  }
}
